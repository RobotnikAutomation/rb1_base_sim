<?xml version='1.0'?>
<sdf version="1.4">
<model name="robotnik_test_ground_v1">
  <pose>0 0 0 0 0 0</pose>
  <static>true</static>

    <link name='robotnik_test_ground_v1'>
	<pose> 0 0 0.030 0 0 0</pose>
		<inertial>
			<mass>0.000001</mass>
		</inertial>
		<collision name="collision">
			<geometry>
			<mesh>
				<uri>model://robotnik_test_ground_v1/meshes/robotnik_test_ground_v1.stl</uri>
			</mesh>
			</geometry>
			<surface>
			  <friction>
				<ode>
					<mu>100</mu>
					<mu2>100</mu2>
				</ode>				
			  </friction>
              <bounce>
                <restitution_coefficient>0.5</restitution_coefficient>
                <threshold>0</threshold>
              </bounce>
              <contact>
                <ode>
                  <max_vel>10</max_vel>
                </ode>
              </contact>
            </surface>			
		</collision>	
		<visual name="visual">
			<geometry>
				<mesh>
					<uri>model://robotnik_test_ground_v1/meshes/robotnik_test_ground_v1.stl</uri>
				</mesh>
			</geometry>
			<material>
				<script>
					<!-- uri>file://media/materials/scripts/gazebo.material</uri -->
					<!-- name>Gazebo/Grey</name -->
					<uri>model://robotnik_test_ground_v1/materials/scripts</uri>
					<uri>model://robotnik_test_ground_v1/materials/textures</uri>	
					<name>Gazebo/TestGround</name>
				</script>
			</material>			
			<transparency>0</transparency>
			<cast_shadows>1</cast_shadows>
		</visual>
	</link>
     
  </model>
</sdf>
